    "@storybook/node-logger": "^6.4.1",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.1",
    "@typescript-eslint/parser": "^5.5.0",
    "commitizen": "^4.2.4",
    "cz-emoji": "^1.3.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
